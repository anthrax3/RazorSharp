@using System.Web.Script.Serialization;
@{
    string chartName = Request["chart"];
    var jsonString = string.Empty;

    AnalyticsUtility analytics = new AnalyticsUtility();

    if (!string.IsNullOrEmpty(chartName))
    {
        ChartDataList chartList = new ChartDataList();
        chartList = analytics.GetChartData(chartName);

        var jsonSerializer = new JavaScriptSerializer();
        string data = jsonSerializer.Serialize(chartList.ListOfChartData);
        jsonString = data;
    }
    else
    {
        jsonString = null;
    }

}
@Html.Raw(jsonString)